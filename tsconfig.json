{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types",
      "vite/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "ignoreWarnings": true,
    "ignoreSyntaxErrors": true,
    "ts-ignore": true,
    "allowJs": true,
    "checkJs": false
  },
}